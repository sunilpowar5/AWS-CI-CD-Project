{
    "family": "my-task-family",
    "networkMode": "awsvpc",
    "containerDefinitions": [
      {
        "name": "my-container",
        "image": "<IMAGE_PLACEHOLDER>",
        "memory": 512,
        "cpu": 256,
        "essential": true,
        "portMappings": [
          {
            "containerPort": 80,
            "hostPort": 80
          }
        ]
      }
    ],
    "requiresCompatibilities": [
      "FARGATE"
    ],
    "cpu": "256",
    "memory": "512",
    "executionRoleArn": "arn:aws:iam::<345594570707>:role/ecsTaskExecutionRole",
    "taskRoleArn": "arn:aws:iam::<345594570707>:role/ecsTaskRole"
  }
  